{
  "debug.internalConsoleOptions": "neverOpen",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "todo-tree.tree.disableCompactFolders": true,
  "files.associations": {
    "*.env.development": "env",
    "*.env.production": "env"
  },
  "docwriter.style": "Auto-detect",
  "json.schemas": [],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.prettier": "explicit"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.eol": "\r\n",
  "prettier.endOfLine": "crlf",
  "vue.splitEditors.layout.left": ["script", "scriptSetup"],
  "vue.splitEditors.layout.right": ["template", "styles", "customBlocks"],
  "vue.splitEditors.icon": true,
  "i18n-ally.localesPaths": ["i18n/resources"],
  "i18n-ally.sourceLanguage": "pl",
  "i18n-ally.displayLanguage": "pl",
  "i18n-ally.keystyle": "flat"
}
